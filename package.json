{
  "name": "diablorun-d2r-client",
  "productName": "Diablo.run",
  "version": "1.0.0",
  "description": "Diablo.run D2R client",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "make-icon": "electron-icon-maker --input=assets/diablo-run-icon.png --output=assets"
  },
  "keywords": [],
  "author": {
    "name": "Leede OÃœ",
    "email": "info@leede.ee"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/icons/win/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "DiabloRun",
            "iconUrl": "C:/Users/Gutnar/Documents/GitHub/diablorun-d2r-client/assets/icons/win/icon.ico",
            "setupIcon": "C:/Users/Gutnar/Documents/GitHub/diablorun-d2r-client/assets/icons/win/icon.ico",
            "skipUpdateIcon": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src * self blob: data: gap:; style-src * self 'unsafe-inline' blob: data: gap:; script-src * 'self' 'unsafe-eval' 'unsafe-inline' blob: data: gap:; object-src * 'self' blob: data: gap:; img-src * self 'unsafe-inline' blob: data: gap:; connect-src self * 'unsafe-inline' blob: data: gap:; frame-src * self blob: data: gap:;",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "css-loader": "^6.7.1",
    "electron": "18.0.1",
    "electron-icon-maker": "^0.0.5",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "electron-fetch": "^1.7.4",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "livesplit-client": "^1.0.2"
  }
}
